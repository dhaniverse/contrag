{
  "name": "contrag",
  "version": "1.0.0",
  "description": "Runtime schema introspection and dynamic entity graph building for multi-relation entities with time series support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "contrag": "dist/cli.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "schema",
    "introspection",
    "entity-graph",
    "vector-store",
    "embedding",
    "postgres",
    "mongodb",
    "time-series"
  ],
  "author": "Dave",
  "license": "MIT",
  "homepage": "https://github.com/dhaniverse/contrag#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhaniverse/contrag.git"
  },
  "bugs": {
    "url": "https://github.com/dhaniverse/contrag/issues"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "contrag": "^1.0.0",
    "dotenv": "^16.3.1",
    "mongodb": "^6.3.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.24.1",
    "ora": "^5.4.1",
    "pg": "^8.11.3",
    "weaviate-ts-client": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.9",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
